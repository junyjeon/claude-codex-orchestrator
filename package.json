    "code-generation",
    "cli-wrapper"
  ],
  "author": "junyjeon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junyjeon/claude-codex-orchestrator.git"
  },
  "bugs": {
    "url": "https://github.com/junyjeon/claude-codex-orchestrator/issues"
  },
  "homepage": "https://github.com/junyjeon/claude-codex-orchestrator#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "dotenv": "^16.5.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
