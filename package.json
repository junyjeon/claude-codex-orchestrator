{
  "name": "@junyjeon/claude-codex-orchestrator",
  "version": "1.0.0",
  "description": "MCP server orchestrating Claude Code and Codex CLI (GPT-5.3) with specialized tools and task routing",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "claude-codex-orchestrator": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
